					<key>COMMAND_STRING</key>
					<string>#!/bin/zsh
# ------------------------------------------------------------------
# V2.0 - Alignement fixe et gestion intelligente des Ko/Mo
# ------------------------------------------------------------------

start_epoch=$(date +%s)

first_item="$1"
parent_dir=$(dirname "$first_item")
parent_name=$(basename "$parent_dir")
nb_total=$#

# Estimation temps
est_secs=$(echo "$nb_total * 0.1" | bc) 
est_secs_int=${est_secs%.*}
if [[ -z "$est_secs_int" || "$est_secs_int" -eq 0 ]]; then est_secs_int=1; fi
estimated_time="${est_secs_int} sec."

# Notification d√©but
title="G√©n√©ration du listing..."
msg="$nb_total √©l√©ments dans : $parent_name"
osascript -e "display notification \"$msg\" with title \"$title\""

date_str=$(date +"%Y%m%d")
temp_dir="$HOME/listing_temp"
mkdir -p "$temp_dir"

# Noms fichiers
listing_base="listing_${date_str}_${parent_name}"
listing_ext=".txt"
listing_final_path="$parent_dir/${listing_base}${listing_ext}"

i=1
while [ -e "$listing_final_path" ]; do
  listing_final_path="$parent_dir/${listing_base}_$i$listing_ext"
  i=$((i+1))
done

listing_temp_path="$temp_dir/$(basename "$listing_final_path")"

# --- CONFIGURATION DU TABLEAU ---
# Format fixe : 20 chars date | 12 chars poids | Reste pour le nom
FMT="%-20s | %-12s | %s\n"
SEP="---------------------|--------------|--------------------------------------------------"

# En-t√™te du fichier
{
    echo "=== LISTING : $parent_name ==="
    echo "G√©n√©r√© le : $(date '+%d/%m/%Y √† %H:%M')"
    echo ""
    printf "$FMT" "Date modif." "Poids" "Nom du fichier"
    echo "$SEP"
} &gt; "$listing_temp_path"

total_files=0
total_size_mo=0
oldest_date=""
current_date_epoch=$(date +%s)

# Boucle principale
for item in "$@"; do
  if [[ -e "$item" ]]; then
    mod_date=$(stat -f "%Sm" -t "%d/%m/%Y %H:%M" "$item" 2&gt;/dev/null || echo "N/A")
    mod_epoch=$(stat -f "%m" "$item")

    # Gestion intelligente du poids (Ko vs Mo)
    if [[ -f "$item" ]]; then
      size_bytes=$(stat -f "%z" "$item")
      
      # Si moins de 1 Mo (1048576 octets), on affiche en Ko
      if (( size_bytes &lt; 1048576 )); then
         size_kb=$(( size_bytes / 1024.0 ))
         size_display=$(printf "%.0f Ko" $size_kb)
         # Si vraiment petit (&lt; 1 Ko)
         if [[ "$size_display" == "0 Ko" ]]; then size_display="&lt; 1 Ko"; fi
      else
         # Sinon en Mo
         size_mo_item=$(echo "scale=2; $size_bytes/1048576" | bc)
         size_display=$(printf "%.2f Mo" $size_mo_item)
      fi

      # Pour le total, on garde tout en Mo pour le calcul
      size_mo_calc=$(echo "scale=4; $size_bytes/1048576" | bc)
      total_files=$((total_files+1))
      total_size_mo=$(echo "$total_size_mo + $size_mo_calc" | bc)
    else
      size_display="[DOSSIER]"
    fi

    item_name=$(basename "$item")

    if [[ -z "$oldest_date" || "$mod_epoch" -lt "$oldest_date" ]]; then
      oldest_date=$mod_epoch
    fi

    # √âcriture de la ligne avec alignement parfait
    printf "$FMT" "$mod_date" "$size_display" "$item_name" &gt;&gt; "$listing_temp_path"
  fi
done

# Pied de page (Fermeture du tableau)
echo "$SEP" &gt;&gt; "$listing_temp_path"

# --- BLOC R√âSUM√â (Hors tableau pour clart√©) ---
age_seconds=$((current_date_epoch - oldest_date))
age_days=$((age_seconds / 86400))
age_months=$((age_days / 30))
age_years=$((age_days / 365))

{
    echo ""
    echo "üìä STATISTIQUES GLOBALES"
    echo "------------------------"
    echo "üìÅ Nombre de fichiers : $total_files"
    echo "üíæ Poids total        : $(printf "%.2f Mo" $total_size_mo)"
    echo "üï∞Ô∏è Anciennet√© max     : $age_days jours ($age_months mois / $age_years ans)"
} &gt;&gt; "$listing_temp_path"

# Finalisation
mv "$listing_temp_path" "$listing_final_path"
rmdir "$temp_dir" 2&gt;/dev/null || true

end_epoch=$(date +%s)
elapsed=$((end_epoch - start_epoch))

# Notification fin propre
title="Listing termin√©"
msg="Fichier cr√©√© : $(basename "$listing_final_path")"
osascript -e "display notification \"$msg\" with title \"$title\""
echo "$listing_final_path"
</string>
